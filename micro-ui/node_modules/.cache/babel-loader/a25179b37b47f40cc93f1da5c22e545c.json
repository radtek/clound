container>\n            <avue-crud :option=\"option\" :data=\"data\"\n                       :page=\"page\"\n                       @row-update=\"handleOpt\"\n                       @row-save=\"handleSav\"\n                       v-model=\"form\">\n                <template slot=\"menuLeft\">\n                    <el-button\n                            class=\"filter-item\"\n                            @click=\"dialogImport = true\"\n                            size=\"small\"\n                            type=\"primary\"\n                            icon=\"el-icon-download\">预览导入\n                    </el-button>\n                </template>\n                <template slot=\"menu\" slot-scope=\"scope\">\n\n                    <el-button\n\n                            size=\"small\"\n                            type=\"text\"\n                            icon=\"el-icon-delete\"\n                            @click=\"handleDelete()\">删除\n                    </el-button>\n                </template>\n                <template slot=\"infoForm\" slot-scope=\"scope\">\n                    <avue-crud :option=\"infoOption\" :data=\"infoData\"\n                               :page=\"page\"\n                               @row-update=\"handleOpt\"\n                               @row-save=\"handleSav\"\n                               @row-del=\"handleDelete()\"\n                    >\n                        <template slot=\"menuLeft\">\n                            <el-button\n                                    class=\"filter-item\"\n                                    @click=\"dialogField = true\"\n                                    size=\"small\"\n                                    type=\"primary\"\n                                    icon=\"el-icon-download\">预览\n                            </el-button>\n                        </template>\n                    </avue-crud>\n                </template>\n            </avue-crud>\n        </basic-container>\n\n        <el-dialog title=\"数据表预览\" :visible.sync=\"dialogImport\">\n            <el-form v-model=\"definedForm\">\n                <el-form-item label=\"来源库\" prop=\"levelId\">\n                    <el-select v-model=\"definedForm.ds\" placeholder=\"请选择来源库\" >\n                        <el-option v-for=\"dict in dsDic\" :label=\"dict.label\" :value=\"dict.value\" :key=\"dict.value\"/>\n                    </el-select>\n                </el-form-item>\n\n                <avue-crud :option=\"expOption\" :data=\"data\"\n                           :page=\"page\"\n                           v-model=\"form\">\n                </avue-crud>\n\n            </el-form>\n            <div slot=\"footer\"\n                 class=\"dialog-footer\">\n                <el-button type=\"primary\" @click=\"importSave\">导 入\n                </el-button>\n            </div>\n        </el-dialog>\n\n        <el-dialog title=\"字段预览\" :visible.sync=\"dialogField\">\n            <el-form v-model=\"definedForm\">\n\n\n                <avue-crud :option=\"fieldOption\" :data=\"fieldData\"\n                           :page=\"page\"\n                           v-model=\"form\">\n                </avue-crud>\n\n            </el-form>\n            <div slot=\"footer\"\n                 class=\"dialog-footer\">\n                <el-button type=\"primary\" @click=\"fieldSave\">导 入\n                </el-button>\n            </div>\n        </el-dialog>\n\n\n    </div>\n</template>\n\n\n<script>\n\n\n    export default {\n        data() {\n            return {\n                page: {\n                    total: 3, // 总页数\n                    currentPage: 1, // 当前页数\n                    pageSize: 20, // 每页显示多少条,\n                    isAsc: false//是否倒序\n                },\n                dialogImport:false,\n                dialogField:false,\n                form: {},\n                definedForm:{\n                    ds:'1',\n\n                },\n                dsDic:[\n                    {\n                        value:'1',\n                        label:'治综资源库'\n                    },\n                    {\n                        value:'2',\n                        label:'警钟资源库'\n                    },\n                    {\n                        value:'3',\n                        label:'旅业库'\n                    }\n                ],\n                 fieldData:[{\n                     fieldCn: '名称',\n                     fieldEn: 'name',\n                     type: 'VARCHAR',\n                     len: '128',\n\n                 }, {\n                     fieldCn: '检查方式',\n                     fieldEn: 'jcfs',\n                     type: 'VARCHAR',\n                     len: '4',\n\n                 },\n                     {\n                         fieldCn: '创建时间',\n                         fieldEn: 'createDate',\n                         type: 'DATE',\n                         len: '0',\n\n                     },\n                     {\n                         fieldCn: '更新时间时间',\n                         fieldEn: 'updateDate',\n                         type: 'DATE',\n                         len: '0',\n\n                     }\n                 ],\n                data: [{\n                    nameCn: '从业人员信息表',\n                    nameEn: 'T_CYRY_INFO',\n                    ds:'1',\n                    createDate: '2019-03-17 17:30',\n                    updateDate: '2019-03-17 17:30',\n                    info: [\n                        {\n                            fieldCn: '姓名',\n                            fieldEn: 'name',\n                            type: 'VARCHAR',\n                            len: '128',\n                            updateDate: '2019-03-17 17:30',\n                        }, {\n                            fieldCn: '性别',\n                            fieldEn: 'sex',\n                            type: 'VARCHAR',\n                            len: '4',\n                            dict:'sex',\n                            updateDate: '2019-03-17 17:30',\n                        }\n                    ]\n                }, {\n                    nameCn: '单位消防检查',\n                    ds:'2',\n                    nameEn: 'T_DWXXFJC_CHECK',\n                    createDate: '2019-03-17 17:30',\n                    updateDate: '2019-03-17 17:30',\n                    info: [\n                        {\n                            fieldCn: '名称',\n                            fieldEn: 'name',\n                            type: 'VARCHAR',\n                            len: '128',\n                            updateDate: '2019-03-17 17:30',\n                        }, {\n                            fieldCn: '消防设施',\n                            fieldEn: 'xfss',\n                            type: 'VARCHAR',\n                            len: '4',\n                            dict:'xfss',\n                            updateDate: '2019-03-17 17:30',\n                        }\n                    ]\n                },\n\n                    {\n                    nameCn: '单位治安检查表',\n                    nameEn: 'T_ZAXFJC_INFO',\n                    ds:'1',\n                    createDate: '2019-03-17 17:30',\n                    updateDate: '2019-03-17 17:30',\n                    info: [\n                        {\n                            fieldCn: '名称',\n                            fieldEn: 'name',\n                            type: 'VARCHAR',\n                            len: '128',\n                            updateDate: '2019-03-17 17:30',\n                        }, {\n                            fieldCn: '检查方式',\n                            fieldEn: 'jcfs',\n                            dict:'jcfs',\n                            type: 'VARCHAR',\n                            len: '4',\n                            updateDate: '2019-03-17 17:30',\n                        }\n                    ]\n                }],\n                option: {\n                    dialogWidth: '100%',\n                    dialogHeight: '100%',\n                    dialogFullscreen: true,\n                    border: true,\n                    index: true,\n                    indexLabel: '序号',\n                    stripe: true,\n                    menuAlign: 'center',\n                    align: 'center',\n                    addBtn: true,\n                    viewBtn: true,\n                    delBtn: false,\n                    column: [\n                        {\n                            label: '来源库',\n                            prop: 'ds',\n                            type:'select',\n                            dicData:[\n                                {\n                                    value:'1',\n                                    label:'治综资源库'\n                                },\n                                {\n                                    value:'2',\n                                    label:'警钟资源库'\n                                },\n                                {\n                                    value:'3',\n                                    label:'旅业库'\n                                }\n                            ],\n                            rules: [{\n                                required: true,\n                                message: '请选择来源库',\n                                trigger: 'blur'\n                            }]\n                        },\n                        {\n                        label: '中文名称',\n                        prop: 'nameCn',\n                        rules: [{\n                            required: true,\n                            message: '请输入表中文名称',\n                            trigger: 'blur'\n                        }]\n                    },\n                        {\n                            label: '英文名称',\n                            prop: 'nameEn',\n                            rules: [{\n                                required: true,\n                                message: '请输入表英文名称',\n                                trigger: 'blur'\n                            }]\n                        },\n                        {\n                            label: '备注',\n                            prop: 'mark',\n                            hide: true,\n                        },\n                        {\n                            label: '创建时间',\n                            prop: 'createDate',\n                            format: 'yyyy-MM-dd HH:mm',\n                            type: 'date',\n                            valueFormat: 'yyyy-MM-dd HH:mm:ss',\n                            editDisplay: false,\n                            addDisplay: false,\n                        },\n                        {\n                            label: '更新时间',\n                            prop: 'updateDate',\n                            type: 'date',\n                            format: 'yyyy-MM-dd HH:mm',\n                            valueFormat: 'yyyy-MM-dd HH:mm',\n                            editDisplay: false,\n                            addDisplay: false,\n                        }\n\n                        , {\n                            labelWidth: 0,\n                            label: '',\n                            prop: 'info',\n                            span: 24,\n                            hide: true,\n                            formslot: true,\n                        }]\n                },\n                infoOption: {\n                    dialogHeight: '100%',\n                    column: [\n                        {\n                            label: '字段中文名称',\n                            labelWidth: 120,\n                            prop: 'fieldCn',\n                            rules: [{\n                                required: true,\n                                message: '请输入字段中文名称',\n                                trigger: 'blur'\n                            }]\n                        },\n                        {\n                            label: '字段英文名称',\n                            labelWidth: 120,\n                            prop: 'fieldEn',\n                            rules: [{\n                                required: true,\n                                message: '请输入字段英文名称',\n                                trigger: 'blur'\n                            }]\n                        },\n                        {\n                            label: '字段类型',\n                            labelWidth: 120,\n                            prop: 'type',\n                            rules: [{\n                                required: true,\n                                message: '请输入字段类型',\n                                trigger: 'blur'\n                            }],\n\n                        },\n                        {\n                            label: '字段长度',\n                            labelWidth: 120,\n                            prop: 'len',\n                            type:'number'\n                        },\n                        {\n                            label: '字典标准',\n                            labelWidth: 120,\n                            prop: 'dict',\n                            type:'select',\n                            dicData:[{\n                                label:'性别-sex',\n                                value:'sex'\n                            },\n                                {\n                                    label:'行政区划-city',\n                                    value:'city'\n                                }\n                                ,\n                                {\n                                    label:'行业类别-hylb',\n                                    value:'hylb'\n                                }\n                                ,\n                                {\n                                    label:'消防设施-xfss',\n                                    value:'xfss'\n                                } ,\n                                {\n                                    label:'检查方式-jcfs',\n                                    value:'jcfs'\n                                }\n                            ],\n                        },\n                        {\n                            label: '备注',\n                            prop: 'mark',\n                            hide: true,\n                        },\n                        {\n                            label: '更新时间',\n                            labelWidth: 120,\n                            prop: 'updateDate',\n                            format: 'yyyy-MM-dd HH:mm',\n                            type: 'date',\n                            valueFormat: 'yyyy-MM-dd HH:mm:ss',\n                            editDisplay: false,\n                            addDisplay: false,\n                        }\n                    ]\n                },\n                expOption:{\n                    expand:false,\n                    refreshBtn:false,\n                    filterBtn:false,\n                    columnBtn:false,\n                    selection: true,\n                    searchBtn:false,\n                    dialogHeight: '100%',\n                    dialogWidth:'80%',\n                    border: true,\n                    index: true,\n                    indexLabel: '序号',\n                    stripe: true,\n                    menuAlign: 'center',\n                    align: 'center',\n                    addBtn: false,\n                    viewBtn: false,\n                    delBtn: false,\n                    editBtn:false,\n                    menu:false,\n                    column: [\n                        {\n                            label: '中文名称',\n                            prop: 'nameCn',\n                            search:'true'\n\n                        },\n                        {\n                            label: '英文名称',\n                            prop: 'nameEn',\n                            search:'true'\n\n                        },\n                        {\n                            label: '备注',\n                            prop: 'mark',\n                            hide: true,\n                        },\n                        {\n                            label: '创建时间',\n                            prop: 'createDate',\n                            format: 'yyyy-MM-dd HH:mm',\n                            type: 'date',\n                            valueFormat: 'yyyy-MM-dd HH:mm:ss',\n                            editDisplay: false,\n                            addDisplay: false,\n                        }\n                    ]\n                },\n                fieldOption:{\n                    expand:false,\n                    refreshBtn:false,\n                    filterBtn:false,\n                    columnBtn:false,\n                    selection: true,\n                    searchBtn:false,\n                    dialogHeight: '100%',\n                    dialogWidth:'80%',\n                    border: true,\n                    index: true,\n                    indexLabel: '序号',\n                    stripe: true,\n                    menuAlign: 'center',\n                    align: 'center',\n                    addBtn: false,\n                    viewBtn: false,\n                    delBtn: false,\n                    editBtn:false,\n                    menu:false,\n                    column: [\n                        {\n                            label: '字段中文名称',\n                            labelWidth: 120,\n                            prop: 'fieldCn'\n\n                        },\n                        {\n                            label: '字段英文名称',\n                            labelWidth: 120,\n                            prop: 'fieldEn',\n                            search:'true'\n\n                        },\n                        {\n                            label: '字段类型',\n                            labelWidth: 120,\n                            prop: 'type'\n\n                        },\n                        {\n                            label: '字段长度',\n                            labelWidth: 120,\n                            prop: 'len',\n                            type:'number'\n                        }\n                    ]\n                }\n            }\n        },\n        computed: {\n            infoData() {\n                return this.form.info\n            }\n        },\n        methods:{\n            handleOpt(row, index, done, loading){\n                done();\n                this.$notify({\n                    title: '成功',\n                    message: '操作成功',\n                    type: 'success',\n                    duration: 2000\n                })\n            },\n            handleSav(row, done, loading){\n                done();\n                this.$notify({\n                    title: '成功',\n                    message: '操作成功',\n                    type: 'success',\n                    duration: 2000\n                })\n            },\n            fieldSave(){\n                this.dialogField=false;\n                this.$message({\n                    showClose: true,\n                    message: '导入成功',\n                    type: 'success'\n                })\n            },\n            importSave(){\n                this.dialogImport=false;\n                this.$message({\n                    showClose: true,\n                    message: '导入成功',\n                    type: 'success'\n                })\n            },\n            handleDelete() {\n                var _this = this\n                this.$confirm('是否确认删除该记录吗\"?', '警告', {\n                    confirmButtonText: '确定',\n                    cancelButtonText: '取消',\n                    type: 'warning'\n                }).then(function () {\n                    _this.$message({\n                        showClose: true,\n                        message: '删除成功',\n                        type: 'success'\n                    })\n                }).catch(function (err) {\n                })\n            },\n        }\n    }\n</script>"],"sourceRoot":"src/views/portal/unit/standard/metadata"}]}