pan:12,\r\n                            dicData: dict.sycslb,\r\n                            rules: [\r\n                                {\r\n                                    required: true,\r\n                                    message: '请输入使用场所类别',\r\n                                    trigger: 'blur'\r\n                                }\r\n                            ]\r\n                        },\r\n                        {\r\n                            label:'评定等级',\r\n                            hide:true,\r\n                            formslot: true,\r\n                            slot: true,\r\n                            prop:'fieldContent',\r\n                            span:24\r\n                        },\r\n                        {\r\n                            label: '说明',\r\n                            prop: 'sm',\r\n                            type: 'textarea',\r\n                            hide: 'true',\r\n                            span:24\r\n                        },\r\n                        {\r\n                            label: '创建人',\r\n                            prop: 'cjr',\r\n                            search:true,\r\n                            editDisabled:'true',\r\n                            span:12,\r\n                        },\r\n                        {\r\n                            label:'创建单位',\r\n                            prop:'cjdw',\r\n                            span:12,\r\n                            rules: [\r\n                                {\r\n                                    required: true,\r\n                                    message: '请输入评定等级机关',\r\n                                    trigger: 'blur'\r\n                                }\r\n                            ]\r\n                        },\r\n                        {\r\n                            label: '时间范围',\r\n                            prop: 'cxsj',\r\n                            type: 'datetimerange',\r\n                  