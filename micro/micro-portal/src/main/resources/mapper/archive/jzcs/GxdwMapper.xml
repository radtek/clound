    <select id="getCount" resultType="integer">
        select COUNT(1) as num from THGL_RES.JWZH_ZA_B_ZA_ZAGXDW A
        <where>
            <if test="dwmc != null and dwmc != ''">
                AND A.qysjjymc like  '%'||#{dwmc}||'%'
            </if>
            <if test="orgCode!=null and orgCode!=''">
                AND  A.DEPARTMENTCODE  like #{orgCode}||'%'
            </if>
            <if test="placeState!=null and placeState!=''">
                AND  A.JYZT  = #{placeState}
            </if>
            <if test="placeType!=null and placeType!=''">
                AND  A.GXHYLX  = #{placeType}
            </if>
              ${dataScope}
        </where>
    </select>

    <select id="findListPage" resultMap="gxdwMap">
        select A.*,'100' AS pointScore ,'01' as pointType from THGL_RES.JWZH_ZA_B_ZA_ZAGXDW A
        <where>

            <if test="query.dwmc != null and query.dwmc != ''">
                AND A.qysjjymc like  '%'||#{query.dwmc}||'%'
            </if>

            <if test="query.orgCode!=null and query.orgCode!=''">
                AND  A.DEPARTMENTCODE  like #{query.orgCode}||'%'
            </if>
            <if test="query.placeState!=null and query.placeState!=''">
                AND  A.JYZT  = #{query.placeState}
            </if>
            <if test="query.placeType!=null and query.placeType!=''">
                AND  A.GXHYLX  = #{query.placeType}
            </if>

            ${query.dataScope}
        </where>
    </select>

    <select id="findOne" resultMap="gxdwMap">

         select A.*,'100' AS pointScore ,'01' as pointType from THGL_RES.JWZH_ZA_B_ZA_ZAGXDW A
         where A.SYSTEMID=#{dwbh} and rownum=1

